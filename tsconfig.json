{
  "compilerOptions": {
    "lib": ["es2019", "dom"],
    "jsx": "react-native",
    "jsxImportSource": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2019",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "build"]
}
